{
  "name": "images-gl",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "vite": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "dev": "bash ./scripts/dev.sh",
    "prepare": "husky",
    "review": "yarn review:frontend && yarn review:rust",
    "fix": "yarn fix:frontend && yarn fix:rust",
    "review:frontend": "eslint . --ext .vue,.js,.ts,.jsx,.tsx && prettier --check .",
    "review:rust": "cd src-tauri && cargo check && cargo clippy",
    "fix:frontend": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix && prettier --write .",
    "fix:rust": "cd src-tauri && cargo clippy --fix --allow-dirty && cargo fmt"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.36.0",
    "@tauri-apps/cli": "^2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "parse-exr": "^1.0.2",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.1.10"
  },
  "description": "高性能的图片加载器 想要达到的效果是 秒开1GB大小的图片 支持png tiff",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lethe-HJ/images-gl.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lethe-HJ/images-gl/issues"
  },
  "homepage": "https://github.com/Lethe-HJ/images-gl#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "config": "commitlint.config.cjs"
  }
}
